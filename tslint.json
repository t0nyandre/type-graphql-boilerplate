{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "no-console": false,
    "object-literal-sort-keys": false,
    "member-access": false,
    "arrow-parens": false,
    "trailing-comma": true,
    "member-ordering": false,
    "prettier": true
  },
  "rulesDirectory": []
}
